{
  "name": "hola-server",
  "version": "3.0.0",
  "type": "module",
  "description": "a meta programming framework used to build nodejs restful api",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "bun run src/index.ts",
    "test": "bun test --timeout 10000 test/**/*.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.2.0",
    "@elysiajs/jwt": "^1.4.0",
    "dateformat": "^5.0.3",
    "elysia": "^1.2.0",
    "mongodb": "^6.12.0",
    "unzipper": "^0.12.3"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hery-node/hola-server.git"
  },
  "keywords": [
    "meta",
    "nodejs",
    "elysia",
    "mongodb",
    "typescript"
  ],
  "author": "hery1979 <hery1979@qq.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hery-node/hola-server/issues"
  },
  "homepage": "https://github.com/hery-node/hola-server#readme"
}