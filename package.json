{
  "name": "hola-server",
  "version": "3.0.0",
  "type": "module",
  "description": "a meta programming framework used to build nodejs restful api",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc",
    "dev": "bun run src/index.ts",
    "test": "bun test --timeout 10000 test/**/*.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run clean && bun run build",
    "publish": "bun publish"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.2.0",
    "@elysiajs/jwt": "^1.4.0",
    "better-sqlite3": "^12.6.2",
    "dateformat": "^5.0.3",
    "elysia": "^1.2.0",
    "mongodb": "^6.12.0",
    "sqlite-vec": "^0.1.7-alpha.2",
    "unzipper": "^0.12.3"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/bun": "latest",
    "typescript": "^5.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hery-node/hola-server.git"
  },
  "keywords": [
    "meta",
    "nodejs",
    "elysia",
    "mongodb",
    "typescript"
  ],
  "author": "hery1979 <hery1979@qq.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hery-node/hola-server/issues"
  },
  "homepage": "https://github.com/hery-node/hola-server#readme"
}